{
  "name": "parcel-vanilla-starter",
  "private": true,
  "version": "0.0.0",
  "source": "src/index.html",
  "targets": {
    "main": {
      "distDir": "dist"
    }
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build dist/index.html",
    "build:css": "tailwindcss build src/styles/tailwind.css -o dist/styles.css",
    "watch:css": "tailwindcss build src/styles/tailwind.css -o dist/styles.css --watch"
  },
  "devDependencies": {
    "parcel": "^2.14.0",
    "postcss": "^8.5.3",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.7"
  },
  "dependencies": {}
}
